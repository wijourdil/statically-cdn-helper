{
    "name": "wijourdil/statically-cdn-helper",
    "description": "A Laravel/Lumen PHP helper to use the statically.io CDN",
    "keywords": [
        "laravel",
        "lumen",
        "statically",
        "statically.io",
        "cdn"
    ],
    "homepage": "https://github.com/wijourdil/statically-cdn-helper",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wijourdil\\Statically\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wijourdil\\Statically\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Wilfried Jourdil",
            "email": "wijourdil@protonmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.6",
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^7.1"
    },
    "prefer-stable": true
}
